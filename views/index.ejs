<html lang="en">

<head>
  <title>
    <%= title %>
  </title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Angular Material style sheet -->
  <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.min.css">
  <link href="https://fonts.googleapis.com/css?family=Roboto+Slab" rel="stylesheet">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="/stylesheets/style.css" />
</head>

<body ng-app="livingSydney" ng-cloak>
  <!--
    Your HTML content here
  -->
  <div ng-controller="AppCtrl" layout="column" layout-fill ng-cloak>
    <section layout="row" flex>
      <md-sidenav class="md-sidenav-left md-whiteframe-4dp" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')" md-whiteframe="4">
        <md-toolbar class="md-theme-indigo">
          <h1 class="md-toolbar-tools"><%= title %></h1>
        </md-toolbar>
        <md-content ng-controller="LeftCtrl">
          <md-button ng-click="close()" class="md-primary" hide-gt-md>
            Close Sidenav Left
          </md-button>
          <p hide show-gt-md>
            This sidenav is locked open on your device. To go back to the default behavior, narrow your display.
          </p>
        </md-content>
      </md-sidenav>
      <md-content flex>
        <md-toolbar class="md-hue-2 md-fixed">
          <div class="md-toolbar-tools">
            <!--<md-button class="md-icon-button" aria-label="Settings" ng-click="toggleLeft()">
              <md-icon class="material-icons">menu</md-icon>
            </md-button>-->
            <md-button class="md-icon-button" aria-label="More" ng-click="currentLocation()">
              <md-icon class="material-icons">location_on</md-icon>
            </md-button>
            <h2>
              <span><%= title %></span>
            </h2>
            <md-autocomplete flex
              ng-disabled="isDisabled"
              md-no-cache="noCache"
              md-selected-item="selectedItem"
              md-search-text-change="searchTextChange(searchText)"
              md-search-text="searchText"
              md-selected-item-change="selectedItemChange(item)"
              md-items="item in querySearch(searchText)"
              md-item-text="item.formatted_address"
              md-min-length="0"
              placeholder="Where would you like to park today?">
              <md-item-template>
                <span md-highlight-text="searchText" md-highlight-flags="^i">{{item.formatted_address}}</span>
              </md-item-template>
              <md-not-found>
                No matching parking locations were found.
              </md-not-found>
            </md-autocomplete>
            <md-button class="md-icon-button" aria-label="More" ng-click="toggleFilterMenu()">
              <md-icon class="material-icons">filter_list</md-icon>
            </md-button>
          </div>
        </md-toolbar>
        <div layout="column" class="sp-filters" ng-show="showFilters">
          <div layout="row" layout-align="left center" class="pad-em">
            <div layout="row" flex="40" class="conditions">
              <md-checkbox ng-model="free" aria-label="Free Parking" flex="20">Free</md-checkbox>
              <md-checkbox ng-model="disability" aria-label="Disability Parking" flex="20">Disability</md-checkbox>
            </div>
            <div layout="row" flex="15">
              <h3>Parking Type:</h3>
            </div>
            <div layout="row" flex>
              <div layout="column" flex>
                <div>
                  <md-slider md-discrete ng-model="parkingType" step="1" min="1" max="4" aria-label="Parking type" flex>
                  </md-slider>
                </div>
                <div layout="row">
                  <span flex="30">
                    <span class="round-num">&lt;1P</span>
                  </span>
                  <span flex="35" class="padleft-dot-five">
                    <span class="round-num">1P</span>
                  </span>
                  <span flex="30">
                    <span class="round-num">2P</span>
                  </span>
                  <span flex>
                    <span class="round-num">2P+</span>
                  </span>
                </div>
              </div>
            </div>
          </div>
          <div layout="row" layout-align="left center">
            <div layout="row" flex="40"></div>
            <div layout="row" flex="10"></div>
          </div>
        </div>
        <div ng-view layout="column" layout-fill layout-align="top center">
        </div>
      </md-content>
    </section>
  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.14.1/locale/en-au.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.14.1/lodash.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/9.1.1/bootstrap-slider.min.js"></script>
  <!-- Angular Material requires Angular.js Libraries -->
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular.min.js"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-route.min.js"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-animate.min.js"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-aria.min.js"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-messages.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-moment/0.10.3/angular-moment.min.js"></script>

  <!-- Angular Material Library -->
  <script src="http://ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDvncyrGON3jeOV1bCmI_DiDZlApRde1d0&libraries=places&"></script>
  <script src="/javascripts/app.js"></script>

</body>

</html>

